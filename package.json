{
  "name": "skeleton-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "server": "nodemon  nodeScript/index",
    "start": "node ./ske && vue-cli-service build && nodemon  nodeScript/index",
    "ske": "node ./ske",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "distDev/*",
      "vue.config.js"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0,
      "no-unused-vars": 0,
      "no-undef": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "css-tree": "^1.1.3",
    "csso": "^4.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "hasha": "^5.2.2",
    "html-minifier": "^4.0.0",
    "html2json": "^1.0.2",
    "js-beautify": "^1.13.5",
    "less-plugin-npm-import": "^2.1.0",
    "lodash": "^4.17.21",
    "memory-fs": "^0.5.0",
    "nodemon": "^2.0.19",
    "opn": "^6.0.0",
    "puppeteer": "^15.5.0",
    "qrcode": "^1.4.4",
    "sockjs": "^0.3.21",
    "sockjs-client": "^1.5.1",
    "vant": "^2.11.0",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "git-revision-webpack-plugin": "^3.0.6",
    "http-proxy-middleware": "^1.1.1",
    "koa": "^2.13.1",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lodash.merge": "^4.6.2",
    "page-skeleton-webpack-plugin": "^0.10.12",
    "rollup": "^2.45.2",
    "vue-color": "^2.8.1",
    "vue-server-renderer": "^2.6.12",
    "vue-skeleton-webpack-plugin": "^1.2.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-log": "^3.0.2",
    "webpack-node-externals": "^2.5.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
